<!-- START row-->
<div class="row">
   <div class="col-xl-6">
      <!-- START card-->
      <div class="card card-default">
         <div class="card-header">Notify Alerts</div>
         <div class="card-body">
            <p>Create toggleable notifications that fade out automatically.</p>
            <p>
               <button class="btn btn-secondary" type="button" data-notify="" data-message="Message ..">Message</button>
            </p>
            <p>You can use HTML inside your notification message</p>
            <p>
               <button class="btn btn-secondary" type="button" data-notify="" data-message="&lt;em class='fa fa-check'&gt;&lt;/em&gt; Message with icon..">Message with Icon</button>
            </p>
         </div>
      </div>
      <!-- END card-->
      <!-- START card-->
      <div class="card card-default">
         <div class="card-header">Alert Styles</div>
         <div class="card-body">
            <div class="alert alert-success" role="alert">
               <strong>Well done!</strong>You successfully read this important alert message.</div>
            <div class="alert alert-info" role="alert">
               <strong>Heads up!</strong>This alert needs your attention, but it&apos;s not super important.</div>
            <div class="alert alert-warning" role="alert">
               <strong>Warning!</strong>Better check yourself, you&apos;re not looking too good.</div>
            <div class="alert alert-danger" role="alert">
               <strong>Oh snap!</strong>Change a few things up and try submitting again.</div>
            <div class="alert alert-warning alert-dismissible fade show" role="alert">
               <button class="close" type="button" data-dismiss="alert" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
               </button>
               <strong>Holy guacamole!</strong>Best check yo self, you&apos;re not looking too good.</div>
         </div>
      </div>
      <!-- END card-->
   </div>
   <div class="col-xl-6">
      <!-- START card-->
      <div class="card card-default">
         <div class="card-header">Notify with Status</div>
         <div class="card-body">
            <p>Uses builtin alert status to apply notifications</p>
            <p>
               <button class="btn btn-danger" type="button" data-notify="" data-message="Message with status.." data-options="{&quot;status&quot;:&quot;danger&quot;}">Danger</button>
               <button class="btn btn-info" type="button" data-notify="" data-message="Message with status.." data-options="{&quot;status&quot;:&quot;info&quot;}">Info</button>
               <button class="btn btn-warning" type="button" data-notify="" data-message="Message with status.." data-options="{&quot;status&quot;:&quot;warning&quot;}">Warning</button>
               <button class="btn btn-success" type="button" data-notify="" data-message="Message with status.." data-options="{&quot;status&quot;:&quot;success&quot;}">Success</button>
            </p>
            <p>Show in different positions</p>
            <p>
               <button class="btn btn-secondary" type="button" data-notify="" data-message="Message in position.." data-options="{&quot;pos&quot;:&quot;bottom-center&quot;}">Bottom Cener</button>
               <button class="btn btn-secondary" type="button" data-notify="" data-message="Message in position.." data-options="{&quot;pos&quot;:&quot;top-left&quot;}">Top Left</button>
               <button class="btn btn-secondary" type="button" data-notify="" data-message="Message in position.." data-options="{&quot;pos&quot;:&quot;top-right&quot;}">Top Right</button>
               <button class="btn btn-secondary" type="button" data-notify="" data-message="Message in position.." data-options="{&quot;status&quot;:&quot;success&quot;, &quot;pos&quot;:&quot;bottom-right&quot;}">Succes at Bottom Right</button>
            </p>
         </div>
      </div>
      <!-- END card-->
      <!-- START card-->
      <div class="card card-default">
         <div class="card-header">Modals</div>
         <div class="card-body">
            <!-- Button trigger modal-->
            <button class="btn btn-primary btn-lg" type="button" data-toggle="modal" data-target="#myModal">Default modal</button>
            <button class="btn btn-primary btn-lg" type="button" data-toggle="modal" data-target="#myModalLarge">Large modal</button>
            <button class="btn btn-primary btn-lg" type="button" data-toggle="modal" data-target="#myModalSmall">Small modal</button>
         </div>
      </div>
      <!-- END card-->
      <!-- START card-->
      <div class="card card-default">
         <div class="card-header">Popovers</div>
         <div class="card-body">
            <button class="btn btn-secondary" type="button" title="Popover title" data-container="body" data-toggle="popover" data-placement="left" data-content="Vivamus sagittis lacus vel augue laoreet rutrum faucibus.">Popover on left</button>
            <button class="btn btn-secondary" type="button" title="Popover title" data-container="body" data-toggle="popover" data-placement="top" data-content="Vivamus sagittis lacus vel augue laoreet rutrum faucibus.">Popover on top</button>
            <button class="btn btn-secondary" type="button" title="Popover title" data-container="body" data-toggle="popover" data-placement="bottom" data-content="Vivamussagittis lacus vel augue laoreet rutrum faucibus.">Popover on bottom</button>
            <button class="btn btn-secondary" type="button" title="Popover title" data-container="body" data-toggle="popover" data-placement="right" data-content="Vivamus sagittis lacus vel augue laoreet rutrum faucibus.">Popover on right</button>
         </div>
      </div>
      <!-- END card-->
      <!-- START card-->
      <div class="card card-default">
         <div class="card-header">Tooltips</div>
         <div class="card-body">
            <button class="btn btn-secondary" type="button" data-toggle="tooltip" data-placement="left" title="Tooltip on left">Tooltip on left</button>
            <button class="btn btn-secondary" type="button" data-toggle="tooltip" data-placement="top" title="Tooltip on top">Tooltip on top</button>
            <button class="btn btn-secondary" type="button" data-toggle="tooltip" data-placement="bottom" title="Tooltip on bottom">Tooltip on bottom</button>
            <button class="btn btn-secondary" type="button" data-toggle="tooltip" data-placement="right" title="Tooltip on right">Tooltip on right</button>
         </div>
      </div>
      <!-- END card-->
   </div>
</div>
